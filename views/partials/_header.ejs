<title>TinyURL</title>

<% if (username) { %>

    <p>Hello <%= username %>!</p>

    <form method='post' action='/logout'>
        <button>Log out</button>
    </form>

<% } else { %>

    <form method='post' action='/login'>
        <input name='username'><button>Sign in</button>
    </form>

<% }; %>
